<project name="Test" basedir=".">
	<target name="test">
		<taskdef classpath="bin" resource="com/blazer/antor/antlib.xml"/>
		<database 
			region="US"
			language="en"
			ref="conn" 
			classpath="ojdbc6_g.jar;orai18n.jar" 
			driver="oracle.jdbc.OracleDriver" 
			url="jdbc:oracle:thin:@localhost:1521:XE" 
			userid="user" 
			password="pass" />
		<data conn="conn" path="data">
			<name condition="id=10">table</name>
		</data>
			
		<table conn="conn" path="table"/>
		<index conn="conn" path="index"/>
		<view conn="conn" path="view"/>
		<sequence conn="conn" path="sequence"/>
		<synonym conn="conn" path="synonym"/>
		<trigger conn="conn" path="trigger"/>
		<procedure conn="conn" path="procedure"/>
		<package conn="conn" path="package"/>
		<package-body conn="conn" path="package-body"/>

	</target>
</project>